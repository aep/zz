using slice;

export fn main() -> int {
    usize mut at = 0;
    u8 mut buf[10] = {0};
    new mut sl = slice::mut_slice::make(buf, static(len(buf)), &at);
    static_assert(sl.size <= len(sl.mem));
    static_assert(len(buf) == len(sl.mem));
    static_assert(sl.size <= 10);
    sl.push(1);
    static_assert(sl.size <= 10);
    sl.push(1);


    return 0;
}
