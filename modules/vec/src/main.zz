using log;
using vec;
using pool;
using err;

export fn main() int {

    new+1000 p = pool::make(8);
    new+0 l = vec::make_with_pool(&p);

    err::assert(l.push("hello"));
    err::assert(l.push_b("bob"));


    err::assert(l.count == 2);
    let mut v = l.items[0];
    log::info(">%.*s<", v.size, v.mem);

    v = l.items[1];
    log::info(">%.*s<", v.size, v.mem);

    return 0;
}
